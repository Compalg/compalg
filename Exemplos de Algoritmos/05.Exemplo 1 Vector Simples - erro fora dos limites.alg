inicio
    escreva "Codigo principal \n"
    inteiro a [ 10 ] <- {1 , 2 , 3 , 4 , 5 , 6 , 7 , 8 , 1 , 1}
    inteiro i
    //erro aqui, para verificar se o captura
    para i de 0 ate 19
        a [ i ] <- a [ i ] * i
    fimpara
    para i de 0 ate 9
        escreva a [ i ] , " "
    fimpara
    
fimalgoritmo

